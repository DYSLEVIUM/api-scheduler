{
  "title": "API Scheduler Overview",
  "tags": [
    "api-scheduler",
    "overview"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "type": "stat",
      "title": "Active Schedules",
      "targets": [
        {
          "expr": "active_schedules_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 50
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "type": "stat",
      "title": "Active Jobs",
      "targets": [
        {
          "expr": "active_jobs_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 100
              },
              {
                "color": "red",
                "value": 500
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "type": "graph",
      "title": "HTTP Request Rate",
      "targets": [
        {
          "expr": "rate(http_requests_total[5m])",
          "legendFormat": "{{method}} {{endpoint}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "type": "graph",
      "title": "HTTP Request Duration (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "{{method}} {{endpoint}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Duration"
        }
      ]
    },
    {
      "id": 5,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "type": "graph",
      "title": "Schedule Executions",
      "targets": [
        {
          "expr": "rate(schedule_executions_total[5m])",
          "legendFormat": "{{schedule_id}} - {{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 24
      },
      "type": "graph",
      "title": "Job Duration",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(job_duration_seconds_bucket[5m]))",
          "legendFormat": "{{schedule_id}} - {{status}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Duration"
        }
      ]
    },
    {
      "id": 7,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 32
      },
      "type": "graph",
      "title": "HTTP Status Codes",
      "targets": [
        {
          "expr": "sum by (status_code) (rate(http_requests_total[5m]))",
          "legendFormat": "{{status_code}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 8,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 32
      },
      "type": "graph",
      "title": "Request/Response Size",
      "targets": [
        {
          "expr": "rate(http_request_size_bytes_sum[5m])",
          "legendFormat": "Request Size",
          "refId": "A"
        },
        {
          "expr": "rate(http_response_size_bytes_sum[5m])",
          "legendFormat": "Response Size",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "bytes",
          "label": "Size"
        }
      ]
    }
  ]
}